service copy-App /system/bin/copyApp.sh
    class late_start
    user root
    group root system
    seclabel u:r:dumpstate:s0
    disabled
    oneshot

on property:persist.vendor.ota.status=download
    start download-ota
    
on property:persist.vendor.ota.status=manualDownload
    start download-ota

service download-ota /system/bin/download.sh
    class late_start
    user root
    seclabel u:r:installd:s0
    group root system
    disabled
    oneshot
    
on property:persist.vendor.ota.status=update
    start update-ota
    
on property:persist.vendor.ota.status=manualUpdate
    start update-ota

service update-ota /system/bin/update.sh
    class late_start
    user root
    seclabel u:r:install_recovery:s0
    group root system
    disabled
    oneshot

on property:persist.vendor.ota.status=updateResult
