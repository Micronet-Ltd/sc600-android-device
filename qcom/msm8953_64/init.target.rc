on property:persist.vendor.ota.status=manualGet
    start get-file

service get-file /system/bin/get.sh
    class late_start
    user root
    seclabel u:r:dumpstate:s0
    group root system
    disabled
    oneshot

on property:vendor.can=*
   write /dev/kmsg "can0"
   write /dev/kmsg ${vendor.can}
   exec u:r:qti_init_shell:s0 -- /vendor/bin/start_can.sh

on property:vendor.can.set_bitrate=*
   write /dev/kmsg "can0 bitrate"
   write /dev/kmsg ${vendor.can.set_bitrate}
   exec u:r:qti_init_shell:s0 -- /vendor/bin/set_can_bitrate.sh

on property:vendor.can.set_op_mode=*
   write /dev/kmsg "can0 listen only"
   write /dev/kmsg ${vendor.can.set_op_mode}
   exec u:r:qti_init_shell:s0 -- /vendor/bin/set_can_mode.sh

on property:vendor.can.masks=*
   write /dev/kmsg "can0 masks"
   write /dev/kmsg ${vendor.can.masks}
   exec u:r:qti_init_shell:s0 -- /vendor/bin/set_can_masks.sh

on property:vendor.can.filters=*
   write /dev/kmsg "can0 filters"
   write /dev/kmsg ${vendor.can.filters}
   exec u:r:qti_init_shell:s0 -- /vendor/bin/set_can_filters.sh

